package com.justmediagroup.ms.auditoriausuario.controller.dto;

import java.util.List;

import com.justmediagroup.logica_comun.patrones.command.IParam;

public class ValidarUsuarioDto implements IParam {

	private UsuarioDto usuario;

	private ValorConfigurableDto valorConfigurable;

	private List<IntentosLoginDto> listadoIntentosLogin;

	/**
	 * @return the usuario
	 */
	public UsuarioDto getUsuario() {
		return usuario;
	}

	/**
	 * @return the listadoIntentosLogin
	 */
	public List<IntentosLoginDto> getListadoIntentosLogin() {
		return listadoIntentosLogin;
	}

	/**
	 * @param listadoIntentosLogin the listadoIntentosLogin to set
	 */
	public void setListadoIntentosLogin(List<IntentosLoginDto> listadoIntentosLogin) {
		this.listadoIntentosLogin = listadoIntentosLogin;
	}

	/**
	 * @param usuario the usuario to set
	 */
	public void setUsuario(UsuarioDto usuario) {
		this.usuario = usuario;
	}

	/**
	 * @return the valorConfigurable
	 */
	public ValorConfigurableDto getValorConfigurable() {
		return valorConfigurable;
	}

	/**
	 * @param valorConfigurable the valorConfigurable to set
	 */
	public void setValorConfigurable(ValorConfigurableDto valorConfigurable) {
		this.valorConfigurable = valorConfigurable;
	}

}
